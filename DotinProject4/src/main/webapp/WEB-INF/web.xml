	</servlet>
	<servlet>
		<servlet-name>BackToForm</servlet-name>
		<display-name>BackToForm</display-name>
		<description></description>
		<servlet-class>com.dotin.controller.BackToForm</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BackToMainPage</servlet-name>
		<display-name>BackToMainPage</display-name>
